<Window x:Class="GACore.DemoApp.Service.FooWizardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GACore.DemoApp.Service"
        xmlns:view="clr-namespace:GACore.DemoApp.View"
        mc:Ignorable="d"
        Title="Foo Wizard Window" Height="450" Width="800">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/GACore.Controls;component/ResourceDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Window.Style>
        <Style BasedOn="{StaticResource MainWindow}" TargetType="Window" />
    </Window.Style>
    <view:FooWizardView/>
</Window>
